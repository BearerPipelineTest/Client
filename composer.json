{
    "name": "toin0u/digitalocean-v2",
    "description": "DigitalOcean API v2 PHP library",
    "keywords": ["DigitalOcean", "API", "Cloud Hosting", "SSD", "VPS"],
    "license": "MIT",
    "authors": [
        {
            "name": "Antoine Corcy",
            "email": "contact@sbin.dk"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Yassir Hannoun",
            "email": "yassir.hannoun@gmail.com"
        },
        {
            "name": "Glenn Jacobs",
            "email": "glenn@neondigital.co.uk"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4.1",
        "guzzlehttp/guzzle": "^6.3|^7.0",
        "kriswallsmith/buzz": "^0.16"
    },
    "suggest": {
        "guzzlehttp/guzzle": "To use GuzzleHttpAdapter, require guzzlehttp/guzzle:6.3|^7.0.",
        "kriswallsmith/buzz": "To use BuzzAdapter, require kriswallsmith/buzz:^0.16."
    },
    "autoload": {
        "psr-4": {
            "DigitalOceanV2\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
