{
    "name": "toin0u/digitalocean-v2",
    "description": "DigitalOcean API v2 client for PHP",
    "keywords": ["DigitalOcean", "API", "Cloud Hosting", "SSD", "VPS"],
    "license": "MIT",
    "authors": [
        {
            "name": "Antoine Corcy",
            "email": "contact@sbin.dk"
        },
        {
            "name": "Graham Campbell",
            "email": "graham@alt-three.com"
        },
        {
            "name": "Yassir Hannoun",
            "email": "yassir.hannoun@gmail.com"
        },
        {
            "name": "Glenn Jacobs",
            "email": "glenn@neondigital.co.uk"
        },
        {
            "name": "Cameron Van Orman",
            "email": "cameron@efelle.com"
        }
    ],
    "require": {
        "php": "^7.1",
        "ext-json": "*",
        "symfony/polyfill-php80": "^1.17"
    },
    "require-dev": {
        "bamarni/composer-bin-plugin": "^1.4.1",
        "guzzlehttp/guzzle": "^6.3.1|^7.0",
        "kriswallsmith/buzz": "^0.16"
    },
    "suggest": {
        "guzzlehttp/guzzle": "To use GuzzleHttpClient, require guzzlehttp/guzzle:^6.3.1|^7.0.",
        "kriswallsmith/buzz": "To use BuzzHttpClient, require kriswallsmith/buzz:^0.16."
    },
    "autoload": {
        "psr-4": {
            "DigitalOceanV2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "DigitalOceanV2\\Tests\\": "tests/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
